<MedicationRequest xmlns="http://hl7.org/fhir">
   <id value="example-rec-04-not-long-acting-opioid-prefetch"/>
   <status value="active"/>
   <intent value="order"/>
   <medicationCodeableConcept>
      <coding>
         <system value="http://www.nlm.nih.gov/research/umls/rxnorm"/>
         <code value="1049502"/>
         <display value="oxyCODONE HCl 10 MG 12HR Extended Release Oral Tablet"/>
      </coding>
   </medicationCodeableConcept>
   <subject>
      <reference value="Patient/example-rec-04-not-long-acting-opioid"/>
   </subject>
   <encounter>
      <reference value="Encounter/example-rec-04-not-long-acting-opioid-prefetch"/>
   </encounter>
   <authoredOn>
      <extension url="http://hl7.org/fhir/StructureDefinition/cqf-expression">
         <valueExpression>
            <language value="text/cql"/>
